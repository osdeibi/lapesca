<!-- INNER-BANNER -->
<div class="inner-banner style-6">
	<%= image_tag 'lapescatamaulipas.jpg', class: 'center-image' %>
	<div class="vertical-align">
		<div class="container">
			<div class="row">
				<div class="col-xs-12 col-md-8 col-md-offset-2">
		  			<ul class="banner-breadcrumb color-white clearfix">
		  				<li><a class="link-blue-2" href="#">galería</a> /</li>
		  				<li><span>fotos</span></li>
		  			</ul>
		  			<h2 class="color-white">la pesca en fotos</h2>
  				</div>
			</div>
		</div>
	</div>
</div>

