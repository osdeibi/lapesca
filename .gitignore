# Ignore bundler config.
/.bundle

# Ignore all logfiles and tempfiles.
/log/*
/tmp/*
!/tmp/.keep

# Ignore Byebug command history file.
.byebug_history

/config/database.yml
/.env
/.ruby-gemset
/.ruby-version

/.rvmrc
/public/uploads/
/electronic_bill/
*.DS_Store
